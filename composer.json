{
  "name": "arraypress/wp-site-utils",
  "version": "1.0.0",
  "description": "A lean WordPress library for site information and configuration management",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-site-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\SiteUtils\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-site-utils/issues",
    "source": "https://github.com/arraypress/wp-site-utils"
  },
  "keywords": [
    "wordpress",
    "site",
    "configuration",
    "wp",
    "arraypress",
    "bloginfo",
    "options",
    "timezone",
    "urls"
  ]
}